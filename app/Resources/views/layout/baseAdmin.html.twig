<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block titre %}Mon site - Admin{% endblock %}</title>
    <meta name="keywords" content="{% block motsCles %}{% endblock %}" />
    <meta name="description" content="{% block description %}{% endblock %}" />
    <link rel="stylesheet" href="{{asset('css/bootstrap.min.css')}}" />
    <link rel="stylesheet" href="{{asset('css/bootstrap-theme.css')}}" />
    <link rel="stylesheet" href="{{asset('css/styleAdmin.css')}}" />
    <link rel="stylesheet" href="{{asset('css/font-awesome.css')}}" />
    <link rel="stylesheet" href="{{asset('css/bttn.css')}}" />
    {% block stylesheets %}{% endblock %}
    <link rel="stylesheet" href="{{asset('css/custom.min.css')}}" />
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>


<body class="nav-md">
<div class="container body">
    <div class="main_container">


        {% include '::admin/sidebar.html.twig' %}
        {% include '::admin/topnav.html.twig' %}


        <!-- page content -->
        <div class="right_col" role="main">
            <div class="">
                {% block top_content_header %}
                    <div class="page-title">
                        <div class="title_left">
                            <h3>{% block page_title %}</h3>Page Title{% endblock page_title %}
                        </div>

                        <div class="title_right">
                            <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Search for...">
                                    <span class="input-group-btn">
                      <button class="btn btn-default" type="button">Go!</button>
                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                {% endblock top_content_header %}

                <div class="clearfix"></div>


                {% block page_content %}{% endblock %}


            </div>
        </div>
        <!-- /page content -->


        {% include 'admin/footer.html.twig' %}


    </div>
</div>


<script src="{{ asset('js/jquery.min.js')}}"></script>
<script src="{{ asset('js/bootstrap.min.js')}}"></script>
<script src="{{ asset('js/custom.min.js')}}"></script>
{% block javascripts %}{% endblock %}
</body>
</html>